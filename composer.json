{
    "name": "montanabay39/mpesa",
    "description": "laravel package for implementing C2B & B2C transactions as prescribed by Safaricom's daraja API",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Wagura Maurice",
            "email": "business@waguramaurice.com"
        }
    ],
    "require": {
        "illuminate/support": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Montanabay39\\Mpesa\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Montanabay39\\Mpesa\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Montanabay39\\Mpesa\\MpesaServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
